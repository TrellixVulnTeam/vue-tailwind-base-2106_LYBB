AL,EAAY;AACR,eAAO,MAAM,CAAC,IAAP,CAAY;AACf,UAAA,IAAI,EAAE;AADS,SAAZ,CAAP;AAGJ,OAPY,CASZ;;;AACA,MAAA,KAAK,CAAC,QAAN,CAAe,OAAf,CAAuB,MAAvB,CAA8B,aAA9B,oBAAwD,KAAxD;AACA,MAAA,KAAK,CAAC,GAAN,CAAU,gCAAV,EACK,IADL,CACU,UAAA,QAAO,EAAK;AAEd;AACA,QAAA,IAAI,CAAC,KAAL,GAAa,QAAQ,CAAC,IAAtB;AAEH,OANL,EAOK,KAPL,CAOW,UAAA,KAAI,EAAK;AACZ,QAAA,OAAO,CAAC,GAAR,CAAY,KAAK,CAAC,QAAN,CAAe,IAA3B;AACH,OATL;AAWH,KAtBQ,CAAT,CAXI,CAmCJ;;AACA,aAAS,MAAT,GAAkB;AAEd;AACA,MAAA,KAAK,CAAC,QAAN,CAAe,OAAf,CAAuB,MAAvB,CAA8B,aAA9B,oBAAwD,KAAxD;AACA,MAAA,KAAK,CAAC,IAAN,CAAW,kCAAX,EACK,IADL,CACU,UAAA,QAAO,EAAK;AAEd,YAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AAEvB;AACA,UAAA,YAAY,CAAC,UAAb,CAAwB,OAAxB,EAHuB,CAKvB;;AACA,iBAAO,MAAM,CAAC,IAAP,CAAY;AACf,YAAA,IAAI,EAAE;AADS,WAAZ,CAAP;AAIJ;AAEH,OAfL,EAgBK,KAhBL,CAgBW,UAAA,KAAI,EAAK;AACZ,QAAA,OAAO,CAAC,GAAR,CAAY,KAAK,CAAC,QAAN,CAAe,IAA3B;AACH,OAlBL;AAoBJ;;AAEA,WAAO;AACH,MAAA,KAAK,EAAL,KADG;AACI;AACP,MAAA,IAAI,EAAJ,IAFG;AAEG;AACN,MAAA,MAAM,EAAN,MAHG,CAGK;;AAHL,KAAP;AAMJ;AAxFW,CAAf","sourcesContent":["<template>\n    <div>\n        <Navigation></Navigation>\n        <Header></Header>\n\n        <div class=\"lg:flex w-full lg:justify-between py-5 px-5\" data-aos=\"zoom-out\" data-aos-delay=\"200\" data-aos-once=\"true\">\n            <div class=\"w-full lg:w-10/12 shadow shadow-lg mr-2\">\n                <Table></Table>\n            </div>\n            <div class=\"lg:w-4/12 mt-5 md:mt0\">\n                <Form></Form>\n            </div>\n        </div>\n\n        <div class=\"px-5 py-5 shadow shadow-lg flex justify-between\">\n            <Modal></Modal>\n            <Modal></Modal>\n            <Modal></Modal>\n        </div>\n        \n\n\n        <!-- <div class=\"py-16 px-4 sm:px-8 sm:py-12 sm:gap-x-8 md:py-16 lg:ml-20\"> -->\n\n        <!-- <button href=\"\"  @click.prevent=\"logout\" class=\"bg-emerald-500 py-3 px-3 rounded-sm block mt-4 lg:inline-block lg:mt-0 text-teal-lighter hover:text-white\">\n        LOGOUT\n      </button> -->\n\n        <!-- Selamat Datang <strong>{{ user.name }}</strong> -->\n        <!-- </div> -->\n\n\n\n    </div>\n</template>\n\n<script>\n    import Navigation from './../../components/Navigation.vue'\n    import Table from './../../components/base/Table.vue'\n    import Form from './../../components/base/Form.vue'\n    import Header from './../../components/admin/Header.vue'\n    import Modal from './../../components/base/Modal.vue'\n    import {\n        onMounted,\n        ref\n    } from 'vue'\n    import {\n        useRouter\n    } from 'vue-router'\n    import axios from 'axios'\n\n    export default {\n        components: {\n            Navigation,\n            Header,\n            Form,\n            Table,\n            Modal,\n        },\n\n  data() {\n    return {\n    //   showModal: false\n    }\n  },\n  methods: {\n    // toggleModal: function(){\n    //   this.showModal = !this.showModal;\n    // }\n  },\n\n        setup() {\n            //state token\n            const token = localStorage.getItem('token')\n\n            //inisialisasi vue router on Composition API\n            const router = useRouter()\n\n            //state user\n            const user = ref('')\n\n            //mounted properti\n            onMounted(() => {\n\n                //check Token exist\n                if (!token) {\n                    return router.push({\n                        name: 'login'\n                    })\n                }\n\n                //get data user\n                axios.defaults.headers.common.Authorization = `Bearer ${token}`\n                axios.get('http://localhost:8000/api/user')\n                    .then(response => {\n\n                        //console.log(response.data.name)\n                        user.value = response.data\n\n                    })\n                    .catch(error => {\n                        console.log(error.response.data)\n                    })\n\n            })\n\n            //method logout\n            function logout() {\n\n                //logout\n                axios.defaults.headers.common.Authorization = `Bearer ${token}`\n                axios.post('http://localhost:8000/api/logout')\n                    .then(response => {\n\n                        if (response.data.success) {\n\n                            //remove localStorage\n                            localStorage.removeItem('token')\n\n                            //redirect ke halaman login\n                            return router.push({\n                                name: 'login'\n                            })\n\n                        }\n\n                    })\n                    .catch(error => {\n                        console.log(error.response.data)\n                    })\n\n            }\n\n            return {\n                token, // <-- state token\n                user, // <-- state user\n                logout, // <-- method logout\n            }\n\n        }\n\n    }\n\n    \n</script>\n<style scoped>\n\n\t\t.animated {\n\t\t\t-webkit-animation-duration: 1s;\n\t\t\tanimation-duration: 1s;\n\t\t\t-webkit-animation-fill-mode: both;\n\t\t\tanimation-fill-mode: both;\n\t\t}\n\n\t\t.animated.faster {\n\t\t\t-webkit-animation-duration: 500ms;\n\t\t\tanimation-duration: 500ms;\n\t\t}\n\n\t\t.fadeIn {\n\t\t\t-webkit-animation-name: fadeIn;\n\t\t\tanimation-name: fadeIn;\n\t\t}\n\n\t\t.fadeOut {\n\t\t\t-webkit-animation-name: fadeOut;\n\t\t\tanimation-name: fadeOut;\n\t\t}\n\n\t\t@keyframes fadeIn {\n\t\t\tfrom {\n\t\t\t\topacity: 0;\n\t\t\t}\n\n\t\t\tto {\n\t\t\t\topacity: 1;\n\t\t\t}\n\t\t}\n\n\t\t@keyframes fadeOut {\n\t\t\tfrom {\n\t\t\t\topacity: 1;\n\t\t\t}\n\n\t\t\tto {\n\t\t\t\topacity: 0;\n\t\t\t}\n\t\t}\n</style>"],"sourceRoot":""}]}